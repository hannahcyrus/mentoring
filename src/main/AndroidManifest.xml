<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.mentoring_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Mentoring_App">
        <activity android:name=".login"></activity>
        <activity android:name=".welcome_cordd" />
        <activity android:name=".welcome_ment_trial" />
        <activity android:name=".welcome_stud" />
        <activity android:name=".forgot_pswd" />
        <activity android:name=".create_acc" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>